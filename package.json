{
  "name": "o-services-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:ts": "eslint --ext \".ts,.tsx,js,jsx\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:ts && yarn lint:style && yarn lint:prettier && next lint",
    "lintfix": "prettier --write --list-different . && yarn lint:ts --fix && yarn lint:style --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache",
    "*.{css,scss,sass,html}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@types/node": "20.5.4",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "autoprefixer": "^10.4.15",
    "eslint": "8.48.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.2",
    "lint-staged": "^15.0.1",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-standard": "^31.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2.2"
  }
}
